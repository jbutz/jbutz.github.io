<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
   {% if page.grouptype == 'tag' %}
   <title>JasonButz.info - {{page.tag | PluralCase}}</title>
   {% elsif page.grouptype == 'category' %}
   <title>JasonButz.info - {{page.category | PluralCase}}</title>
   {% else %}
   <title>JasonButz.info</title>
   {% endif %}
   <link>http://www.jasonbutz.info</link>
   <description/>
   <language>en-us</language>
   <atom:link href="http://www.jasonbutz.info/{{page.category}}/atom.xml" rel="self" type="application/rss+xml" />
   
   {% for post in page.posts %}
   <item>
    <title>{{ post.title }}</title>
    <link>http://www.jasonbutz.info{{ post.url }}</link>
    <dc:creator  xmlns:dc="http://purl.org/dc/elements/1.1/">Jason Butz</dc:creator>
    <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
    <guid>http://www.jasonbutz.info{{ post.url }}</guid>
    <description><![CDATA[
     {{ post.content | expand_urls : site.url }}
     ]]></description>
   </item>
   {% endfor %}
 </channel>
</rss>